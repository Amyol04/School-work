public class ex1 {
	public static void main(String[] args) {
		
		String carName ="volvo";
		int maxSpeed =120;
		
		
		int x= 5;
		int y= 10;
		
		
		int z = x + y;
		
		System.out.println("Car Name: " +carName);
		System.out.println("Max Speed; " + maxSpeed); // using the + character to connect String and variable values 
		
		
		System.out.println("x is :" +x); //
		System.out.println("y is :" +y);
		
		
		
		
		System.out.println("z is :" +z);	// using + to add nums together 
	
		
		
	} // end of class
		
} // end of main
